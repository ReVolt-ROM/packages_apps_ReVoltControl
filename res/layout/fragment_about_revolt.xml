<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:revolt="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/scrollView">

    <LinearLayout
            style="@style/settingsFragmentStyle">

        <com.revolt.control.widgets.DeveloperPreference
                revolt:nameDev="ReVolt"
                revolt:twitterHandle="ReVolt-ROM"
                revolt:githubLink="@string/github_revolt"
                revolt:donateLink="@string/donate_revolt"
                revolt:emailDev="johnhany97@gmail.com"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>


        <com.revolt.control.settings.UrlSetting
                android:id="@+id/revolt"
                android:title="@string/revolt_website_title"
                android:summary="@string/revolt_website_summary"
                revolt:url="@string/url_revolt"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

    </LinearLayout>
</ScrollView>
